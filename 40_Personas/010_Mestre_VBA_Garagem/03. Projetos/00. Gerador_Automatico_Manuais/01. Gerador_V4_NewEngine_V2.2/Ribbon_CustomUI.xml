<!-- INICIO DA SEÇÃO PALETIZADOR (ID 8) -->
<!-- Cole este bloco após a tag </tab> do CCMX e antes de </tabs> -->

<tab id="PLT8" label="Paletizador" insertAfterMso="CCMX">
	<group id="GInf8" label="Projeto">
		<button id="Inf8" label="Informações Gerais" size="large" onAction="Inform" image="project" />
	</group>
	<group id="Preenc8" label="Preenchimento">
		<button id="Comp8" label="Componentes Integrados" size="large" onAction="Comp" image="machine-learning" />
		
		<!-- Botões de Navegação Específicos PLT (Mec8, El8, IHM8) -->
		<button id="Mec8" label="Estrutura / Mecânica" size="large" onAction="Mec_PLT" image="mechanism" />
		<button id="Dados8" label="Dados Técnicos" size="large" onAction="Dados_PLT" image="mechanism" />
		<button id="El8" label="Componentes Elétricos" size="large" onAction="El_PLT" image="fuse-box" />
		<button id="IHM8" label="IHM" size="large" onAction="IHM_PLT" image="panel" />
		
		<!-- Exceções removido se não existir, ou manter genérico -->
		<button id="Ex8" label="Exceções e Especificações" size="large" onAction="Ex7" image="danger" />
		
		<separator id="sep8" />
		<button id="Pr8" label="Exemplos de preenchimento" size="large" onAction="Preenchimento" image="filling-cabinet" />
	</group>
	<group id="GroupGer8" label="Geradores">
		<!-- Gatilho da Nova Engine -->
		<button id="Ger8" label="Gerar Manual PLT" size="large" onAction="Gerar_PLT_Click" image="storage-tank" />
		
		<button id="GIHM8" label="Atualização de IHM" size="large" onAction="GIMH_PLT" image="technology" />
		<button id="Im8" label="Substituir Imagens" size="large" onAction="SImagem" image="replace" />
		<button id="Tab8" label="Substituir Tabelas" size="large" onAction="STabela" image="table" />
	</group>
	<group id="GLimp8" label="Limpeza">
		<button id="Limp8" label="Limpar Página" size="large" onAction="Limp" image="clean" />
	</group>
    <!-- Identificação removida conforme solicitado -->
</tab>

